<p id="notice"><%= notice %></p>
<main class="wrapper">
  <header>
    <h2>Products</h2>
  </header>
  
  <div class="container-fluid">
    <div class="row">
    	<% @products.each do |product| %>
				<% if product.image_url != nil %>
        	<%= render @products %>
      	<% end %>
			<% end %>
    </div> <!-- end of row -->
  </div> <!-- end of container -->
  
</main>




